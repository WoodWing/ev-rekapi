{
    "rules": {
        "linebreak-style": [
            2,
            "unix"
        ],
        "no-empty": [
            2
        ],
        "indent": [
            2,
            4,
            { SwitchCase: 1 }
        ],
        "quotes": [
            2,
            "double"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars": [
            2,
            {
                "vars": "all",
                "args": "none"
            }
        ],
        "no-console": 1
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "globals": {
        "angular"
    },
    "extends": "eslint:recommended"
}
